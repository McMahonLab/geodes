x <- read.csv(file.choose(), header = T)
counts <- table(x$taxonomy)
counts
counts <- as.data.frame(counts)
counts
library(ggplot2)
library(cowplot)
ggplot(data = counts, aex(x = Var, y = Freq)) + geom_bar(stat = "identity") + theme(axis.text.x = element_text(angle = 90, hjust = 1))
